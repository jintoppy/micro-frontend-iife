  "repository": "https://us.tools.publicis.sapient.com/stash/projects/CGDMP/repos/sdp-prototypes",
  "scripts": {
    "clean": "rimraf dist",
    "copy:css": "cross-conf-env ncp dist/ npm_package_config_clientlibs --filter=npm_package_config_nonjspattern",
