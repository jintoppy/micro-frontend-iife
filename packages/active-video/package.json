{
  "name": "@sdp/active-video",
  "version": "1.0.0",
  "private": true,
  "description": "Atomic Components - ActiveVideo",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=8.11.3",
    "npm": "5.6.0"
